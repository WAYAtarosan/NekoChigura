# coding: utf-8
n,s,t,p = map(int,input().split())
ans = []
ad_list = {}
for i in range(1,n+1):
    tmp = int(input())
    a = list(map(int,input().split()))
    ad_list[i] = a

def dfs(v,path):
    if v == t and p in path:
        ans.append(tuple(path))
    else:
        for j in ad_list[v]:
            if j not in path:
                path.append(j)
                dfs(j,path)
                path.pop()
dfs(s,[s])
print(len(ans))
for l in ans:
    print(*l)

            
