#一度通った頂点は通らない（対義はウォーク）
n, s, k = map(int, input().split())
ad_list = {}
for i in range(1, n + 1):
    a = list(range(1, n + 1))
    a.remove(i)
    ad_list[i] = a

import random

path = [s]
for _ in range(k):
    next_s = list(set(ad_list[s]) - set(path))
    s = random.choice(next_s)
    path.append(s)

print(*path)
